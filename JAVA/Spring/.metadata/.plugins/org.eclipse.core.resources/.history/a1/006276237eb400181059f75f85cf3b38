public class ObjectMasterSamurai extends ObjectMasterHuman {
    ObjectMasterSamurai(){
        setHP(200);
        setStr(5);
    }

    public ObjectMasterHuman deathBlow(ObjectMasterHuman target){
        target.setHP(0);
        setHP(getHP() / 2);
        System.out.println("You annihilated your target. You how have only: " + getHP() + " health left.");
        return target;
    }

    public void meditate(){
        setHP(200);
        System.out.println("You feel refreshed.");
        return;
    }

    public void howMany(){
        int count = 0;
        for(int i = 0; i < ObjectMasterSamurai; i++){
            count++;
        }
        System.out.println(count);
        return;
    }

}
